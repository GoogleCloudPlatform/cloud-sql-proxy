{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignorePresets": [":semanticPrefixFixDepsChoreOthers"],
  "prConcurrentLimit": 0,
  "rebaseStalePrs": true,
  "dependencyDashboard": true,
  "semanticCommits": true,
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "ignoreDeps": [
      "golang.org/x/net"
  ],
  "timezone": "America/Los_Angeles",
  "schedule": [
    "after 8am on Friday",
    "before 12pm on Friday"
  ],
  "force": {
    "constraints": {
      "go": "1.16"
    }
  }
}
