{
  "baseBranches": [
    "main",
    "v1"
  ],
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignorePresets": [
      ":semanticPrefixFixDepsChoreOthers"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["golang.org/x/net"],
      "enabled": false,
      "matchBaseBranches": ["v1"]
    }
  ],
  "prConcurrentLimit": 0,
  "rebaseStalePrs": true,
  "dependencyDashboard": true,
  "semanticCommits": true,
  "postUpdateOptions": [
    "gomodTidy"
  ]
}
