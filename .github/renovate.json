{
  "extends": [
    "config:recommended"
  ],
  "dependencyDashboardLabels": ["type: process"],
  "commitMessagePrefix": "deps: ",
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "prConcurrentLimit": 3,
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "groupName": "bump dependencies for github"
    },
    {
      "matchManagers": ["dockerfile"],
      "groupName": "bump container images"
    }
  ]
}
