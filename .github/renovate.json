{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignorePresets": [":semanticPrefixFixDepsChoreOthers"],
  "prConcurrentLimit": 0,
  "rebaseStalePrs": true,
  "dependencyDashboard": true,
  "semanticCommits": true,
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "ignoreDeps": [
    "golang.org/x/net",
    "google.golang.org/api"
  ],
  "force": {
    "constraints": {
      "go": "1.16"
    }
  }
}
